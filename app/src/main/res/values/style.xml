<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mytoast">
        <item name="stTextBold">true</item>
        <item name="stTextColor">@color/white</item>
        <item name="stTextSize">18sp</item>
        <item name="stColorBackground">@color/green</item>
        <item name="stSolidBackground">true</item>
        <item name="stStrokeWidth">2dp</item>
        <item name="stStrokeColor">@color/black</item>
        <item name="stIconStart">@drawable/success</item>
<!--        <item name="stIconEnd">@drawable/ic</item>-->
        <item name="stLength">LONG</item> LONG or SHORT
        <item name="stGravity">center</item> top or center
        <item name="stRadius">8dp</item>
    </style>

    <style name="mytoasterror">
        <item name="stTextBold">true</item>
        <item name="stTextColor">@color/white</item>
        <item name="stTextSize">18sp</item>
        <item name="stColorBackground">@color/red</item>
        <item name="stSolidBackground">true</item>
        <item name="stStrokeWidth">2dp</item>
        <item name="stStrokeColor">@color/black</item>
        <item name="stIconStart">@drawable/error</item>
        <!--        <item name="stIconEnd">@drawable/ic</item>-->
        <item name="stLength">LONG</item> LONG or SHORT
        <item name="stGravity">center</item> top or center
        <item name="stRadius">8dp</item>
    </style>





</resources>